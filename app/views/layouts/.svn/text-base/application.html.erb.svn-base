<!DOCTYPE html>
<html>
<head>
  <title>apps</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

	<div class="container">
		<% flash.each do |key,msg| %>
			<div class="alert alert-<%= key%>">
				<a class="close" href="#" data-dismiss="alert">&times;</a><%= msg %>
			</div>
		<% end %>
		<%= yield %>
	</div>


</body>
</html>
