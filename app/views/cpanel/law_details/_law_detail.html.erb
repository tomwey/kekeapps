<% cache law_detail do %>
<tr>
	<td><%= link_to law_detail.id, [:cpanel, law_detail], class: 'badge badge-info' %></td>
	<td width="15%"><%= law_detail.title %></td>
	<td width="10%"><%= law_detail.law_type_name %></td>
	<td><%= law_detail.version.to_s %></td>
	<td><%= law_detail.category_name %></td>
	<td width="12%"><%= law_detail.app_title %></td>
	<td width="10%"><%= law_detail.law_udid %></td>
	<td width="5%"><%= law_detail.action %></td>
	<td width="22%"><%= law_detail.created_at.strftime('%Y-%m-%d %H:%M:%S') %></td>
	<td width="15%">
		<%= link_to '编辑', edit_cpanel_law_detail_path(law_detail), class: 'btn btn-mini' %>
		<%= link_to '删除', [:cpanel, law_detail], method: :delete, confirm: '确定要删除吗？', class: 'btn btn-danger btn-mini' %>
	</td>
</tr>
<% end %>